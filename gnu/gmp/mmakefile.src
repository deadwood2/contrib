include $(SRCDIR)/config/aros-contrib.cfg

#MM- contrib-gnu-gmp : development-gmp

GMP_VERSION=6.3.0

GMP_EXTRAOPTS := \
    --libdir=$(AROS_LIB)

GMP_CPPFLAGS :=
GMP_CFLAGS :=  $(CFLAGS_GNU99) $(GMP_CPPFLAGS) 

%fetch_and_build_gnu_development package=gmp version=$(GMP_VERSION) patch=yes \
     extraoptions="$(GMP_EXTRAOPTS)" extracflags="$(GMP_CFLAGS)" nlsflag=no xflag=no

%common
