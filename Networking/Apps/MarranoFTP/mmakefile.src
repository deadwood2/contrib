# Makefile for MarranoFTP

include $(SRCDIR)/config/aros-contrib.cfg

FILES  := main

EXEDIR := $(AROS_CONTRIB)/Networking/Apps/MarranoFTP

NOWARN_FLAGS := \
    $(NOWARN_UNINITIALIZED) \
    $(NOWARN_PARENTHESES) \
    $(NOWARN_FORMAT_OVERFLOW) \
    $(NOWARN_STRINGOP_TRUNCATION)

USER_CFLAGS := $(CFLAGS_GNU99) $(NOWARN_FLAGS)
USER_CPPFLAGS := -D__BSD_VISIBLE=1

